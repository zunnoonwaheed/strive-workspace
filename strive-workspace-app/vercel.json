{
  "buildCommand": "cd strive-workspace-app && npm run build",
  "outputDirectory": "strive-workspace-app/dist",
  "devCommand": "cd strive-workspace-app && npm run dev",
  "installCommand": "cd strive-workspace-app && npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
